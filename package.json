{
  "name": "liqpass-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 100",
    "format": "prettier --write .",
    "test": "vitest --config vitest.config.ts",
    "test:watch": "vitest watch --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "abi:liqpass": "cd contracts && forge build && forge inspect LiqPassBinaryMarket abi --json > abi/LiqPassBinaryMarket.json"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.19",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.44.4",
    "wagmi": "^2.19.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.21",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^4.0.16"
  }
}
